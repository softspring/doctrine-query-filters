{
    "name": "softspring/doctrine-query-filters",
    "description": "This library adds a new filter for doctrine queries",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": false,
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.11",
        "symfony/form": "^5.4|^6.0|^7.0",
        "symfony/http-foundation": "^5.4|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "Softspring\\Component\\DoctrineQueryFilters\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\Component\\DoctrineQueryFilters\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
